<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ls="http://learnsphere.org/ls" >

  <xs:include schemaLocation="../../CommonSchemas/WorkflowsCommon.xsd" />
  

  <xs:complexType name="InputDefinition0">
    <xs:complexContent>
      <xs:extension base="InputContainer">
        <xs:sequence>
          <xs:element type="InFileList0" name="files" />
              <xs:any minOccurs="0" processContents="skip" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="InFileList0">
    <xs:choice>
      <xs:element ref="file"  minOccurs="0" />
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="InputType">
    <xs:all>
      <xs:element name="input0" type="InputDefinition0" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="InputLabel">
    <xs:all>
      <xs:element name="input0" type="xs:string" default="tab-delimited" minOccurs="0" />
    </xs:all>
  </xs:complexType>

  <xs:complexType name="OutputDefinition0">
    <xs:complexContent>
      <xs:extension base="InputContainer">
        <xs:sequence>
          <xs:element type="OutFileList0" name="files" />
          <xs:any minOccurs="0" processContents="skip" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="OutFileList0">
    <xs:choice>
      <xs:element ref="tab-delimited" />
    </xs:choice>
  </xs:complexType>


  <xs:complexType name="OutputType">
    <xs:sequence>
      <xs:element name="output0" type="OutputDefinition0" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="columnOperationType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Add a column" />
      <xs:enumeration value="Change column names" />
      <xs:enumeration value="Cumulative sum" />
      <xs:enumeration value="Remove columns" />
      <xs:enumeration value="Sort" />
      <xs:enumeration value="Unique value" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="valueWithEmptyType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NA" />
      <xs:enumeration value="A constant" />
      <xs:enumeration value="Value from column" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="valueWithNoEmptyType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="A constant" />
      <xs:enumeration value="Value from column" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="cumsumValueType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Count within group" />
      <xs:enumeration value="Column cumulative sum within group" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="operationType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Add" />
      <xs:enumeration value="Subtract" />
      <xs:enumeration value="Multiply" />
      <xs:enumeration value="Divide" />
      <xs:enumeration value="Concatenate" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="trueFalseType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="No" />
      <xs:enumeration value="Yes" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="sortOrderType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Ascending" />
      <xs:enumeration value="Descending" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="nullType" final="restriction">
    <xs:restriction base="xs:string">
      <xs:enumeration value="blank" />
      <xs:enumeration value="." />
	  <xs:enumeration value="NA" />
	  <xs:enumeration value="NONE" />
	  <xs:enumeration value="NULL" />
    </xs:restriction>
  </xs:simpleType>
  
  

